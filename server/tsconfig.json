{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "moduleResolution": "node",
    "esModuleInterop": false,
    "target": "es6",
    "module": "commonjs",
    //"moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["../client/src/*"]
    }
  },
  "include": ["next-env.d.ts", "../client/src/**/*.ts", "../client/src/**/*.tsx", "server.mjs", "server.js"],
  "exclude": ["node_modules"]
}
